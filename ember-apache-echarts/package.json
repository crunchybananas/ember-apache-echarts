    "ember-template-lint": "5.13.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-ember": "11.12.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.0.1",
