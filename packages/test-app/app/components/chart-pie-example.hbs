<h2>Simple Pie Chart</h2>
<Chart::Pie
  class="border"
  @width="100%"
  @height="200"
  @data={{this.chartData}}
  @noDataText="No data"
/>

<h2>Simple Donut Chart</h2>
<Chart::Pie
  class="border"
  @width="100%"
  @height="200"
  @variant="donut"
  @data={{this.chartData}}
  @noDataText="No data"
/>